{
  "name": "lheadsup-play",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "copy_data": "mkdir data -p && cp ./node_modules/lheadsup/data/HandRanks.dat data/",
    "build": "rimraf dist && esbuild ./src/main.ts --platform=node --format=esm --bundle --outdir=dist --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\" && tsc --emitDeclarationOnly",
    "lint:watch": "tsc --noEmit --watch",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "bench": "vitest bench",
    "start": "node ./dist/main.js",
    "train:mix": "node ./dist/main.js --train=mix",
    "train:flop": "node ./dist/main.js --train=flop",
    "train:prod": "node ./dist/main.js --train=mix -b 100 -c 6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tensorflow/tfjs": "^4.6.0",
    "esbuild": "^0.17.16",
    "lheadsup": "link:../lheadsup",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "devDependencies": {
    "@types/node": "^20.3.1"
  }
}
